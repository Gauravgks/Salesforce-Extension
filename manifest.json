{
  "name": "Salesforce Extension",
  "version": "1.0.0",
  "description": "Salesforce Extension",
  "manifest_version": 3,
  "author": "Gaurav",
  "action": {
    "default_popup": "index.html",
    "default_title": "Salesforce Extension"
  },
  "permissions": ["tabs"],
  "host_permissions": ["https://*.app.github.dev/*", "http://localhost:8080/*"],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "script.js"],
      "matches": ["https://*/*", "http://*/*", "chrome-extension://*/*"]
    }
  ]
}
